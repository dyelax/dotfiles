alias gb="git branch | cat"
alias gmom="git fetch; git merge origin/main"
alias gdel="git branch -D"
alias gdelrem="git push origin --delete"
grename() { git branch -m $1 $2; git push origin :$1 $2; git push origin -u $2; }
alias gstc="git stash clear"
alias gst="git stash"
alias gs="git status"
alias gcom="git checkout main"
alias gcob="git checkout -b"
alias gcaa="git commit -a --amend"
alias gcm="git commit -m"
alias gcam="git commit -am"
alias gds="git diff --name-status"

alias gprv="gh pr view --web"
alias gprc="gh pr create --web"

